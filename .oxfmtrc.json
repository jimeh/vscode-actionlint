{
  "$schema": "./node_modules/oxfmt/configuration_schema.json",
  "printWidth": 80,
  "semi": true,
  "singleQuote": false,
  "trailingComma": "all",
  "ignorePatterns": [
    "*.vsix",
    "CHANGELOG.md",
    ".claude/",
    ".vscode-test/",
    ".vscode/settings.json",
    "dist/",
    "node_modules/",
    "out/",
    "package.json",
    "pnpm-lock.yaml"
  ]
}
